{
  "version": 2,
  "name": "gob-apps",
  "rewrites": [
    {
      "source": "/beta-combined-dashboard.html",
      "destination": "/public/beta-combined-dashboard.html"
    },
    {
      "source": "/test-simple.html",
      "destination": "/public/test-simple.html"
    },
    {
      "source": "/simple-dashboard.html",
      "destination": "/public/simple-dashboard.html"
    },
    {
      "source": "/test-page.html",
      "destination": "/public/test-page.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        }
      ]
    }
  ]
}
