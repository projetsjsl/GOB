    "api/cron/fmp-batch-sync.js": {
      "maxDuration": 300
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET,POST,OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/calendar-earnings",
      "destination": "/api/fmp?endpoint=earnings-calendar"
    },
    {
      "source": "/api/calendar-economic",
      "destination": "/api/fmp?endpoint=economic-calendar"
    },
    {
      "source": "/api/marketdata",
      "destination": "/api/fmp"
    },
    {
      "source": "/api/finnhub",
      "destination": "/api/marketdata"
    },
    {
      "source": "/",
      "has": [{ "type": "host", "value": "jlab.jslai.app" }],
      "destination": "/jlab-settings.html"
    },
    {
      "source": "/dashboard",
      "has": [{ "type": "host", "value": "jlab.jslai.app" }],
      "destination": "/jlab-dashboard.html"
    },
    {
      "source": "/hub",
      "has": [{ "type": "host", "value": "jlab.jslai.app" }],
      "destination": "/jlab.html"
    },
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "jlaj.jslai.app" }],
      "destination": "/jlab-dashboard.html"
    }
  ],
  "crons": [],
  "redirects": [
    {
      "source": "/test",
      "destination": "/test-simple.html",
      "permanent": false
    },
    {
      "source": "/reee",
      "destination": "https://jslai.app/reee",
      "permanent": false
    },
    {
      "source": "/REEE",
      "destination": "https://jslai.app/reee",
      "permanent": false
    },
    {
      "source": "/eval",
      "destination": "https://jslai.app/evaluation",
      "permanent": false
    },
    {
      "source": "/nouveauclient",
      "destination": "/nouveauclient/index.html",
      "permanent": false
    },
    {
      "source": "/3p1",
      "destination": "/3p1/dist/index.html",
      "permanent": false
    },
    {
      "source": "/3p1/",
      "destination": "/3p1/dist/index.html",
      "permanent": false
    },
    {
      "source": "/jlab",
      "destination": "/beta-combined-dashboard.html",
      "permanent": false
    },
    {
      "source": "/jlab/",
      "destination": "/beta-combined-dashboard.html",
      "permanent": false
    },
    {
      "source": "/bienvenue",
      "destination": "/bienvenue/index.html",
      "permanent": false
    },
    {
      "source": "/bienvenue/",
      "destination": "/bienvenue/index.html",
      "permanent": false
    }
  ]
}