{
  "functions": {
    "api/gemini/chat.js": {
      "maxDuration": 60,
      "memory": 1024
    },
    "api/gemini/chat-validated.js": {
      "maxDuration": 60,
      "memory": 1024
    },
    "api/marketdata.js": {
      "maxDuration": 15
    },
    "api/news.js": {
      "maxDuration": 15
    },
    "api/fmp.js": {
      "maxDuration": 15
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET,POST,OPTIONS" },
        { "key": "Access-Control-Allow-Headers", "value": "Content-Type" }
      ]
    }
  ],
  "rewrites": [],
  "redirects": []
}