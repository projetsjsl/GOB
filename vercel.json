{
  "version": 2,
  "functions": {
    "api/marketdata.js": {
      "maxDuration": 10
    },
    "api/marketdata/batch.js": {
      "maxDuration": 30
    },
    "api/gemini/chat.js": {
      "maxDuration": 30
    },
    "api/emma-agent.js": {
      "maxDuration": 300
    },
    "api/emma-briefing.js": {
      "maxDuration": 45
    },
    "api/supabase-watchlist.js": {
      "maxDuration": 15
    },
    "api/config/tickers.js": {
      "maxDuration": 10
    },
    "api/calendar-economic.js": {
      "maxDuration": 15,
      "memory": 1024
    },
    "api/calendar-earnings.js": {
      "maxDuration": 15,
      "memory": 1024
    },
    "api/calendar-dividends.js": {
      "maxDuration": 15,
      "memory": 1024
    },
    "api/ai-services.js": {
      "maxDuration": 120
    },
    "api/fmp.js": {
      "maxDuration": 10
    },
    "api/finviz-news.js": {
      "maxDuration": 15
    },
    "api/chat.js": {
      "maxDuration": 60
    },
    "api/adapters/sms.js": {
      "maxDuration": 30
    },
    "api/adapters/email.js": {
      "maxDuration": 30
    },
    "api/adapters/messenger.js": {
      "maxDuration": 30
    }
  },
  "crons": [
    {
      "path": "/api/briefing-cron",
      "schedule": "20 11 * * 1-5"
    },
    {
      "path": "/api/briefing-cron",
      "schedule": "50 15 * * 1-5"
    },
    {
      "path": "/api/briefing-cron",
      "schedule": "20 20 * * 1-5"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET,POST,OPTIONS" },
        { "key": "Access-Control-Allow-Headers", "value": "Content-Type" }
      ]
    }
  ],
  "rewrites": [
    { "source": "/api/finnhub", "destination": "/api/marketdata" }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "/beta-combined-dashboard.html",
      "permanent": false
    },
    {
      "source": "/index.html",
      "destination": "/beta-combined-dashboard.html",
      "permanent": false
    },
    {
      "source": "/test",
      "destination": "/test-simple.html",
      "permanent": false
    }
  ]
}