      "description": "Prix et quotes en temps réel via FMP (300 calls/min, plan Starter)",
      "category": "financial_data",
      "priority": 1,
      "enabled": true,
      "parameters": {
        "ticker": {
          "type": "string",
          "required": true,
          "description": "Symbole de l'action (ex: AAPL, GOOGL)"
