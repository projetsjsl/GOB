      "description": "Récupère les prix en temps réel des actions via Polygon.io",
      "category": "financial_data",
      "priority": 1,
      "enabled": true,
      "parameters": {
        "ticker": {
          "type": "string",
          "required": true,
          "description": "Symbole de l'action (ex: AAPL, GOOGL)"
        }
      },
      "implementation": {
        "file": "polygon-stock-price-tool.js",
        "endpoint": "https://api.polygon.io/v2/aggs/ticker/{ticker}/prev",
