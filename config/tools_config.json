    {
      "id": "rsi-screener",
      "name": "RSI Screener",
      "description": "Identifie les opportunites RSI extremes (survente: RSI(14)<=20 ET RSI(5)<=5, surachat: RSI(14)>=80 ET RSI(5)>=95) sur marches internationaux (US, CA, EU)",
      "category": "screening",
      "priority": 2,
      "enabled": true,
      "parameters": {
        "type": {
          "type": "string",
          "required": false,
          "enum": ["oversold", "overbought", "both"],
          "default": "both",
          "description": "Type de recherche: oversold (survente), overbought (surachat), both (les deux)"
        },
        "markets": {
          "type": "array",
          "required": false,
          "default": ["US"],
          "description": "Marches a scanner: US, CA, UK, FR, DE, EU"
        },
        "limit": {
          "type": "number",
          "required": false,
          "default": 20,
          "description": "Nombre max de resultats par categorie"
        },
        "market_cap": {
          "type": "string",
          "required": false,
          "enum": ["large", "mid", "all"],
          "default": "large",
          "description": "Capitalisation minimum"
        }
      },
      "implementation": {
        "file": "rsi-screener.js",
        "function": "screenByRSI",
        "endpoint": "/api/rsi-screener",
        "api_key_env": ["FMP_API_KEY", "TWELVE_DATA_API_KEY"]
      },
      "fallback_tools": [],
      "usage_context": ["rsi", "survente", "surachat", "oversold", "overbought", "extreme", "extreme", "indicateur", "technique", "opportunite"],
      "keywords": ["rsi", "survente", "surachat", "oversold", "overbought", "extreme", "extreme", "indicateur", "technique", "technical", "momentum", "opportunite", "opportunity", "trading", "swing"]
    },
