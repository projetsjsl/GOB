          "type": "string",
          "required": false,
          "description": "Secteur spécifique (Technology, Healthcare, etc.)"
        }
      },
      "implementation": {
        "file": "stock-screener.js",
        "function": "searchStocks",
        "api_key_env": ["PERPLEXITY_API_KEY", "FMP_API_KEY"]
      },
      "fallback_tools": [],
      "usage_context": ["screening", "recherche", "trouve", "cherche", "liste", "recommande", "suggère", "identifie", "top", "meilleurs"],
