{
  "name": "gob-dashboard",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:vercel": "vercel dev --listen 127.0.0.1:3001",
    "dev:vite": "vite",
    "build": "npm run build:rgl && npm run build:tailwind-public && vite build",
    "build:tailwind-public": "tailwindcss -i ./src/tailwind-standalone.css -o ./public/css/tailwind.css --minify",
    "build:vite": "vite build",
    "build:rgl": "esbuild src/react-grid-layout-bridge.js --bundle --outfile=public/js/react-grid-layout-bundle.js --format=iife --global-name=ReactGridLayout --external:react --external:react-dom --minify",
    "preview": "vite preview",
    "lint": "eslint src/ api/ --ext .js,.ts,.tsx",
    "lint:fix": "eslint src/ api/ --ext .js,.ts,.tsx --fix",
    "lint:strict": "eslint src/ api/ --ext .js,.ts,.tsx --max-warnings 0",
    "lint:dashboard": "node scripts/check-dashboard-jsx.js",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "qa": "npm run lint && npm run build",
    "verify-3p1": "node scripts/verify-3p1-build.js",
    "test-3p1": "cd public/3p1 && npm run build && cd ../.. && node scripts/verify-3p1-build.js",
    "build:dashboard": "esbuild public/js/dashboard/app-inline.js --bundle --outfile=public/js/dashboard/app-inline.compiled.js --format=iife --loader:.js=jsx --target=es2020 --minify"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.17.0",
    "@browserbasehq/stagehand": "^2.5.2",
    "@google/generative-ai": "^0.21.0",
    "@octokit/rest": "^20.0.0",
    "@supabase/supabase-js": "^2.87.1",
    "@vercel/functions": "^3.3.4",
    "axios": "^1.13.2",
    "body-parser": "^2.2.0",
    "dotenv": "^17.2.3",
    "esbuild": "^0.27.2",
    "express": "^5.1.0",
    "formidable": "^3.5.4",
    "playwright-core": "^1.40.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-grid-layout": "^2.1.1",
    "resend": "^3.0.0",
    "twilio": "^4.20.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/standalone": "^7.23.6",
    "@swc/cli": "^0.7.9",
    "@swc/core": "^1.15.7",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.57.0",
    "pg": "^8.16.3",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "xlsx": "^0.18.5"
  },
  "engines": {
    "node": "22.x"
  }
}
